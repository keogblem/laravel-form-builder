{
    "name": "keogblem/laravel-form-tools",
    "description": "This package helps generate HTML Forms from JSON",
    "keywords": [
        "generators",
        "laravel",
        "form",
        "html",
        "helper"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier GBLEM",
            "email": "olivier@keographix.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "KeoGblem\\FormTools\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeoGblem\\FormTools\\FormToolsServiceProvider"
            ],
            "aliases": {
                "FormTools": "KeoGblem\\FormTools\\FormToolsFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
